{
    "name": "generator-oasp",
    "version": "2.0.0",
    "description": "Yeoman generator for AngularJS applications based on OASP4JS",
    "license": "Apache License 2.0",
    "main": "app/index.js",
    "repository": "oasp/generator-oasp",
    "author": "The OASP Team",
    "engines": {
        "node": ">=0.10.0"
    },
    "scripts": {
        "test": "mocha -t 300000",
        "test-no-generate": "env no-generate=true mocha -t 0",
        "test-log-gulp": "env log-gulp=true mocha -t 0",
        "test-no-generate-log-gulp": "env log-gulp=true, no-generate=true mocha -t 0"
    },
    "files": [
        "app",
        "controller",
        "dialog",
        "directive",
        "module",
        "oasp-base-generator.js"
    ],
    "keywords": [
        "yeoman-generator",
        "angular",
        "template"
    ],
    "dependencies": {
        "chalk": "1.1.0",
        "mkdirp": "0.5.1",
        "ng-parse-module": "0.1.0",
        "string": "3.3.0",
        "underscore": "1.8.3",
        "underscore.string": "3.1.1",
        "yeoman-generator": "0.20.2",
        "yosay": "1.0.5",
        "fs-extra": "0.22.1"
    },
    "devDependencies": {
        "bower": "1.4.1",
        "chai": "3.2.0",
        "cross-spawn": "2.0.0",
        "glob": "5.0.14",
        "mocha": "2.2.5",
        "rimraf": "2.4.1"
    },
    "peerDependencies": {
        "yo": ">=1.0.0"
    }
}
